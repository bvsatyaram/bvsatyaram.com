<h2>Sign in</h2>
<div id="login_form_container">
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:id => "login_form", :class => "clearfix"}) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me, :class => 'rememberme' %>
      <%= f.label :remember_me, :class => 'rememberme' %>
    <% end -%>

    <%= f.submit "Sign in", :class => "button" %>
  <% end %>
</div>

<%#= render :partial => "devise/shared/links" %>