<div id="welcome">
  <img src="/images/bvsatyaram.jpg" />
  <div class="message">
    <h2>Hey! Welcome to my home page!!!</h2>
    <div id="about_info">
      <%= render :partial => "home/about_info" %>
    </div>
    <%= link_to "read more >>", about_path, :class => "more" %>
  </div>
</div>

<div id="welcome_actions" class="clearfix">
  <a class="action first" href=<%=ExternalLinks::BLOG_URL%>>
    <span class="info">Let me speak...</span>
    <img src="/images/home/blog.jpg" />
    <span class="caption">
      <span class="title">Blog</span>
    </span>
  </a>
  <a class="action" href=<%=ExternalLinks::ARTICLES_URL%>>
    <span class="info">Let me share...</span>
    <img src="/images/home/articles.jpg" />
    <span class="caption">
      <span class="title">Articles</span></span>
  </a>
  <a class="action" href=<%=projects_path%>>
    <span class="info">Let me stage...</span>
    <img src="/images/home/projects.jpg" />
    <span class="caption">
      <span class="title">Projects</span>
    </span>
  </a>
  <a class="action" href=<%=contact_path%>>
    <span class="info">Let us meet...</span>
    <img src="/images/home/contact.jpg" />
    <span class="caption">
      <span class="title">Contact</span></span>
  </a>
</div>